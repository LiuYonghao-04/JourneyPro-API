# Server
PORT=3001

# MySQL
DB_HOST=localhost
DB_PORT=3306
DB_USER=root
DB_PASS=123456
DB_NAME=journeypro
DB_POOL_SIZE=10

# Auth
JWT_SECRET=journeypro-secret

# Runtime safety / observability
ENABLE_RUNTIME_SCHEMA_MIGRATION=0
SLOW_API_MS=800
METRIC_SAMPLE_LIMIT=400
OPS_METRICS_TOKEN=

# Routing (OSRM)
OSRM_URL=http://localhost:5000
OSRM_URLS=http://localhost:5000,https://router.project-osrm.org
OSRM_ENABLE_PUBLIC_FALLBACK=1
OSRM_LOCAL_TIMEOUT_MS=600
OSRM_REMOTE_TIMEOUT_MS=12000
OSRM_DOWN_COOLDOWN_MS=15000

# Reco bandit fast history path
BANDIT_HISTORY_CACHE_TTL_MS=120000
# BANDIT_USE_EVENT_HISTORY=0
